<h1>Listing songs</h1>

<table>
  <tr>
    <th>User</th>
    <th>Name</th>
    <th>Performer</th>
    <th>Sounds like</th>
    <th>Lyrics</th>
    <th>Guitar tab</th>
    <th>Bass tabs</th>
    <th>Youtube url</th>
    <th>Midi file url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @songs.each do |song| %>
  <tr>
    <td><%= song.user_id %></td>
    <td><%= song.name %></td>
    <td><%= song.performer %></td>
    <td><%= song.sounds_like %></td>
    <td><%= song.lyrics %></td>
    <td><%= song.guitar_tab %></td>
    <td><%= song.bass_tabs %></td>
    <td><%= song.youtube_url %></td>
    <td><%= song.midi_file_url %></td>
    <td><%= link_to 'Show', song %></td>
    <td><%= link_to 'Edit', edit_song_path(song) %></td>
    <td><%= link_to 'Destroy', song, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Song', new_song_path %>
