<%- model_class = Preset -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @preset.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @preset.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:url) %>:</strong></dt>
  <dd><%= @preset.url %></dd>
  <dt><strong><%= model_class.human_attribute_name(:instrument_id) %>:</strong></dt>
  <dd><%= @preset.instrument_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tenant_id) %>:</strong></dt>
  <dd><%= @preset.tenant_id %></dd>
</dl>

<div class="form-actions">
  
  <%= link_to t('.edit', :default => t("<i class='icon-music'></i> Audion")),
    edit_preset_path(@preset), :class => 'btn btn-success' %>
  
  <%= link_to t('.edit', :default => t("<i class='icon-pencil'></i> Add to rack")),
    edit_preset_path(@preset), :class => 'btn btn-success' %>
  
   
  <%= link_to t('.edit', :default => t("<i class='icon-pencil'></i> Rate")),
    edit_preset_path(@preset), :class => 'btn btn-success' %>
  
  <%= link_to t('.back', :default => t("helpers.links.back")),
    presets_path, :class => 'btn'  %>
  
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
    edit_preset_path(@preset), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
    preset_path(@preset),
    :method => 'delete',
    :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
    :class => 'btn btn-danger' %>
</div>


<% content_for :sidebar do  %>
  <h5> Recommended Presets </h5>
  <%= render 'shared/fake_presets_list'%>
<% end %>